/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below
*/

/* Global */
@site       : 'logamberkah';
@reset      : 'logamberkah';

/* Elements */
@button     : 'logamberkah';
@container  : 'logamberkah';
@divider    : 'logamberkah';
@emoji      : 'logamberkah';
@flag       : 'logamberkah';
@header     : 'logamberkah';
@icon       : 'logamberkah';
@image      : 'logamberkah';
@input      : 'logamberkah';
@label      : 'logamberkah';
@list       : 'logamberkah';
@loader     : 'logamberkah';
@placeholder: 'logamberkah';
@rail       : 'logamberkah';
@reveal     : 'logamberkah';
@segment    : 'logamberkah';
@step       : 'logamberkah';
@text       : 'logamberkah';

/* Collections */
@breadcrumb : 'logamberkah';
@form       : 'logamberkah';
@grid       : 'logamberkah';
@menu       : 'logamberkah';
@message    : 'logamberkah';
@table      : 'logamberkah';

/* Modules */
@accordion  : 'logamberkah';
@calendar   : 'logamberkah';
@checkbox   : 'logamberkah';
@dimmer     : 'logamberkah';
@dropdown   : 'logamberkah';
@embed      : 'logamberkah';
@modal      : 'logamberkah';
@nag        : 'logamberkah';
@popup      : 'logamberkah';
@progress   : 'logamberkah';
@slider     : 'logamberkah';
@rating     : 'logamberkah';
@search     : 'logamberkah';
@shape      : 'logamberkah';
@sidebar    : 'logamberkah';
@sticky     : 'logamberkah';
@tab        : 'logamberkah';
@toast      : 'logamberkah';
@transition : 'logamberkah';

/* Views */
@ad         : 'logamberkah';
@card       : 'logamberkah';
@comment    : 'logamberkah';
@feed       : 'logamberkah';
@item       : 'logamberkah';
@statistic  : 'logamberkah';

/*******************************
            Folders
*******************************/

/* Path to theme packages */
@themesFolder : 'themes';

/* Path to site override folder */
@siteFolder  : 'site';


/*******************************
         Import Theme
*******************************/

@import (multiple) "theme.less";

/* End Config */
